{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1",
            "headers": {
                "X-Frame-Options": "ALLOW"
            }
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "index, follow"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "functions": {},
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ]
}